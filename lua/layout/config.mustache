var Publish = {
	center: {lat: {{center.lat}}, long: {{center.long}}},
	zoom: {{zoom}},
	minZoom: {{minZoom}},
	maxZoom: {{maxZoom}},
	mapTypeId: "{{base}}",
	path: (function(){
		var data = "./data/";
		var url = "{{path}}";
		if(url)
			return data + url + "/";
		return data;
	}()),
	data: {
		cells: {
			river: {
			{{#color}}
				{{key}}: {{#quotes}}{{rgba}}{{/quotes}},
			{{/color}}
			}
		}
	}
};